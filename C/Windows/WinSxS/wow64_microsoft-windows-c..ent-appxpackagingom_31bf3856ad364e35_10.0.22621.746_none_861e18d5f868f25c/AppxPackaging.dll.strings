      <xs:enumeration value="manual"/>
      <xs:enumeration value="disabled"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CT_TriggerEvents">
    <xs:choice minOccurs="0" maxOccurs="20000">
